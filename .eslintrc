{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ],
  "globals": {
    "__DEV__": true
  },
  "env": {
    "browser": true
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  },
  "rules": {
    "no-shadow": 1,
    "prefer-const": 0,
    "jsx-quotes": 0,
    "prefer-rest-params":0,
    "react/prop-types": 0,
    "react/no-deprecated": 1,
    "react/display-name": 1
  }
}
